{
  "name": "express-mongoose-ra-json-server",
  "version": "0.1.0",
  "description": "creates express.js routes from a mongoose model for ra-data-json-server",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/NathanAdhitya/express-mongoose-ra-json-server.git"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest",
    "clean": "rimraf ./dist/",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src"
  },
  "keywords": [
    "express",
    "mongoose",
    "ra-data-json-server"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "Nathan Adhitya",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "jest": "^29.6.1",
    "mongodb-memory-server": "^8.13.0",
    "mongoose": "^7.3.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^3.0.0",
    "mongoose": "^7.0.0 || ^6.0.0"
  },
  "dependencies": {
    "escape-string-regexp": "^5.0.0"
  }
}
