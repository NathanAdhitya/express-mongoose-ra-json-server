{
  "name": "express-mongoose-ra-json-server",
  "version": "0.0.2",
  "description": "creates express.js routes from a mongoose model for ra-data-json-server",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/NathanAdhitya/express-mongoose-ra-json-server.git"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src"
  },
  "keywords": [
    "express",
    "mongoose",
    "ra-data-json-server"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "Nathan Adhitya",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^3.0.0",
    "mongoose": "^6.0.0 || ^5.0.0"
  },
  "dependencies": {
    "escape-string-regexp": "^4.0.0"
  }
}
