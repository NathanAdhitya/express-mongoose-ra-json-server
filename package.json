{
  "name": "express-mongoose-ra-json-server",
  "version": "0.0.1",
  "description": "creates express.js routes from a mongoose model for ra-data-json-server",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NathanAdhitya/express-mongoose-ra-json-server.git"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "build:cjs": "tsc --outDir dist/cjs",
    "build:esm": "tsc --outDir dist/esm --module es2015",
    "watch": "tsc --outDir dist/cjs --watch"
  },
  "keywords": [
    "express",
    "mongoose",
    "ra-data-json-server"
  ],
  "files": [
    "dist"
  ],
  "author": "Nathan Adhitya",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^3.0.0",
    "mongoose": "^6.0.0 || ^5.0.0"
  },
  "dependencies": {
    "escape-string-regexp": "^4.0.0"
  }
}
